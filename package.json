{
  "name": "zshared",
  "description": "useful common javascript/typescript utilities I collected over the years",
  "version": "1.0.2",
  "main": "lib",
  "types": "lib",
  "module": "es",
  "exports": {
    ".": [
      {
        "import": "./es/index.mjs",
        "require": "./lib/index.js",
        "default": "./lib/index.js"
      },
      "./lib/index.js"
    ]
  },
  "scripts": {
    "build": "ts-hybrid-esm-build --clean",
    "old-build": "tsc",
    "preversion": "npm run lint",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zohar1000/zutilities.git"
  },
  "files": [
    "lib/**/*",
    "es/**/*"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zohar1000/zutilities/issues"
  },
  "homepage": "https://github.com/zohar1000/zutilities",
  "devDependencies": {
    "ts-hybrid-esm-build": "^0.2.3"
  }
}
